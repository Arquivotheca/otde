.SUFFIXES: .cpp

CXX = @CXX@
MOC = @MOC@
INCFLAGS = -I. @all_includes@ -I.. -I../..
CXXFLAGS = -Wall $(INCFLAGS) -DHAVE_CONFIG_H
LDFLAGS = @all_libraries@ -lqt -lkfm -lkdecore -lkhtmlw -lXext -lX11 @LIBSOCKET@ @LIBCOMPAT@ -lgif -ljpeg

########## Implicit Rules ##########

.cpp.o:
	$(CXX) $(CXXFLAGS) $(DEFS) -c $<

########## Files ##########

SRCS = kdeindex.cpp
SRCMETA =
HDRS =
OBJS = kdeindex.o

TARGET = helpindex

########## Build Rules ##########

all: $(TARGET)

$(TARGET): $(SRCMETA) $(OBJS)
	$(CXX) -o $(TARGET) $(OBJS) $(LDFLAGS)

clean:
	rm -f *~ *.bak *.BAK $(TARGET) $(SRCMETA) $(OBJS) core

depend: $(SRCS)
	@makedepend $(INCFLAGS) $(SRCS) 2> /dev/null

########## Meta objects ##########

%.moc: %.h
	$(MOC) $< -o $@

