
INCDIR  =       @all_includes@ -I../.. -I./
CXXFLAGS+=      $(INCDIR) 
LDFLAGS =       @all_libraries@ -lqt -lXext -lX11 -lkdecore @LIBSOCKET@
CXX     =       @CXX@
MOC     =       @MOC@

####### Files

SOURCES =	main.cpp
HEADERS =	
OBJECTS =	main.o
SRCMETA =	
TARGET	=	kfmwarn

####### Implicit rules

.SUFFIXES: .cpp

.cpp.o:
	$(CXX) -c $(CXXFLAGS) -DHAVE_CONFIG_H $(INCDIR) $<

####### Build rules

all: $(TARGET)

$(TARGET): $(SRCMETA) $(OBJECTS)
	$(CXX) $(OBJECTS) -o $(TARGET) $(LDFLAGS)

depend:
	@makedepend $(INCDIR) $(SOURCES) 2> /dev/null

showfiles:
	@echo $(SOURCES) $(HEADERS) Makefile

clean:
	-rm -f *.o *.bak *~ *% #*
	-rm -f $(SRCMETA) $(TARGET)


####### Meta objects

kioslave_ipc.moc: kioslave_ipc.h
	$(MOC) kioslave_ipc.h -o kioslave_ipc.moc

main.moc: main.h
	$(MOC) main.h -o main.moc

