top_srcdir = @top_srcdir@
DESTDIR = @prefix@/mimetypes
INSTALL_DIR = $(SHELL) $(top_srcdir)/mkinstalldirs
INSTALL_DATA = @INSTALL@ -m 644

all:

Makefile: Makefile.in
	cd ../.. && CONFIG_FILES=kfm/mimetypes/$@ CONFIG_HEADERS= $(SHELL) ./config.status

install:
	$(INSTALL_DIR) $(DESTDIR)
	@for i in $(APPLNKS); do \
	 echo "$(INSTALL_DATA) $$i $(DESTDIR)"; \
	 test -d $(DESTDIR)/`dirname "$$i"` \
	 || $(INSTALL_DIR) $(DESTDIR)/`dirname "$$i"`; \
	 $(INSTALL_DATA) "$$i" $(DESTDIR)/"$$i"; \
	done

uninstall:
	for i in $(APPLNKS); do \
	 rm -f $(DESTDIR)/"$$i"; \
	done

