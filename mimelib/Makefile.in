	rm -rf .deps

.deps/%.P: $(srcdir)/%.c
	@echo "Computing dependencies for $<..."
	@o='o';				\
	test -n "$o" && o='$$o';	\
	$(MKDEP) $< | sed "s,^\(.*\)\.o:,\1.$$o \1.l$$o $@:," > $@

.deps/%.P: $(srcdir)/%.cpp
	@echo "Computing dependencies for $<..."
	@o='o';				\
	$(CXXMKDEP) $< | sed "s,^\(.*\)\.o:,\1.$$o \1.l$$o $@:," > $@
