#: main.cpp:420
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"
msgstr ""
"Utilisation: %s -install|-setup|-test|-desc|-preview wid\n"
"             [-corners xxxx] [-delay num] [-lock] [-nice num]\n"

#: main.cpp:423
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Placer le curseur dans le coin provoque:\n"
"                     x = i  aucune action (ignore)\n"
"                     x = s  sauvegarde le bureau\n"
"                     x = l  vérouille l'écran\n"
"                    ordre: haut-guauche, haut-droite, bas-gauche, bas-droite\n"
"  -delay num        Nombre de minutes d'inactivité avant l'activation de la\n"
"                    mise en veille (standard 10min)\n"
"  -desc             Imprime la description de la mise en veille\n"
"  -install          Installe la mise en veille\n"
"  -lock             Nécessite un mot de pass pour déverouiller la \n"
"                    mise en veille\n"
"  -nice num         Est lancé avec le niveau nice \"num\"\n"
"\n"
"  -preview wid      Est lancé dans la fenêtre spécifiée\n"
"  -setup            Instal\n"
"  -test             Lance la mise en veille immédiatement\n"

