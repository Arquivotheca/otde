#: main.C:1155
msgid "Permitted arguments are:\n"
msgstr "Les arguments reconnus sont :\n"

#: main.C:1156
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <commande> <arg> ...   exécute 'commande' avec les paramètres 'args' - "
"dernier argument absolument\n"

#: main.C:1157
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <name>           précise l'écran (serveur)\n"

#: main.C:1158
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>       taille initiale de la fenêtre\n"

#: main.C:1159
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr ""
"-print-pipe <name>        précise la redirection pour imprimante vt100\n"

#: main.C:1160
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <colour>           couleur d'arrière plan\n"

#: main.C:1161
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <colour>           couleur d'avant-plan\n"

#: main.C:1162
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <fontname>       police normale\n"

#: main.C:1163
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <size>           tiny, small, normal, large, huge\n"

#: main.C:1164
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr ""
"-linux                    configure kvt de façon à imiter la console Linux\n"

#: main.C:1165
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               cache la barre de menu\n"

#: main.C:1166
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             cache la barre de défilement\n"

#: main.C:1167
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <text>                 texte dans la barre de titre\n"
"                          (Obsolète. Utilisez -caption à la place)\n"

#: main.C:1169
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Type de terminal ('xterm' par défaut).\n"

#: main.C:1170
msgid "-C                        Capture system console message\n"
msgstr "-C                        Capture les messages système de la console\n"

#: main.C:1171
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <text>                 nom de l'icône ou fenêtre d'icône\n"

#: main.C:1172
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        fonctionne en mode 7 bits\n"
"-8                        fonctionne en mode 8 bits\n"

#: main.C:1174
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       initialise kvt en login shell\n"
"-ls-                      initialise kvt en non-login shell (par défaut)\n"

#: main.C:1176
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arg>               gère Meta avec le préfixe ESCAPE, 8THBIT (8ème bit "
"positionné) ou ignore\n"

#: main.C:1177
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr ""
"-sl <number>              nombre de lignes enregistrées pour retour arrière\n"

#: main.C:1178
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <keysym>          utilise la séquence alt-keysym pour remonter dans "
"le tampon\n"
"-pagedown <keysym>        utilise la séquence alt-keysym pour avancer dans "
"le tampon\n"

#: main.C:1182
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tclavier grec = ELOT 928 (défaut)\n"
"-grk4\t\tclavier grec = IBM 437\n"
