#: main.C:1439
msgid "Permitted arguments are:\n"
msgstr "Lovlige argumenter er:\n"

#: main.C:1440
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <kommando> <arg> ...   kjør kommando med ars - må være siste argument\n"

#: main.C:1441
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <navn>          oppgi skjerm(tjener)\n"

#: main.C:1442
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spes>       vt-vinduets geometry ved start\n"

#: main.C:1443
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <navn>        oppgi rør for vt100-skriver\n"

#: main.C:1444
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <farge>            bakgrunnsfarge\n"

#: main.C:1445
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <farge>            forgrunnsfarge\n"

#: main.C:1446
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <skrifttype>     vanlig skrifttype\n"

#: main.C:1447
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr ""
"-no_font_hack             skru av koden som går utenom Qt-skrifttypefeilen\n"

#: main.C:1448
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <størrelse>      tiny, small, normal, large, huge\n"

#: main.C:1449
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr ""
"-linux                    sett opp kvt til å etterligne linux-konsolet\n"

#: main.C:1450
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               skjul menylinje\n"

#: main.C:1451
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             skjul rullefelt\n"

#: main.C:1452
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <tekst>                tekst i tittellinjen til vinduet \n"
"                          (Foreldret. Bruk -caption isteden)\n"

#: main.C:1454
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Terminaltype. Standard er xterm.\n"

#: main.C:1455
msgid "-C                        Capture system console message\n"
msgstr "-C                        Vis systemkonsollmeldinger\n"

#: main.C:1456
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <tekst>                navn på ikon eller ikon-vindu\n"

#: main.C:1457
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        kjør i 7-bitsmodus\n"
"-8                        kjør i 8-bitsmodus\n"

#: main.C:1459
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       initsialiser kvts skall som et logginnskall\n"
"-ls-                      initsialiser kvts skall som et                     "
"      ikke-logginnskall (standard)\n"

#: main.C:1461
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arg>               behandle Meta med ESCAPE-prefiks, 8bits eller "
"ignorer\n"

#: main.C:1462
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <nummer>              lagre nummer linjer i rullelinjeminnet\n"

#: main.C:1463
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <tastsym>         bruk hurtigtast alt-tastsym for å rulle opp "
"bufferbuffer\n"
"-pagedown <keysym>        bruk hurtigtast alt-tastsym for å rulle ned "
"buffer\n"

#: main.C:1467
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tgreek kbd = ELOT 928 (standard)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
