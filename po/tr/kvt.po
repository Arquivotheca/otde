#: main.C:1326
msgid "Permitted arguments are:\n"
msgstr "Ýzin verilen parametreler:\n"

#: main.C:1327
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <command> <arg> ...    parametreler ile komut çalýþtýrýr - Son parametre olmalýdýr\n"

#: main.C:1328
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <ad>             Görüntü uçbirimini tanýmlar\n"

#: main.C:1329
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>       vt penceresi geometrisini ayarlar\n"

#: main.C:1330
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <ad>          vt100 yazýcýlar için pipe tanýmlar\n"

