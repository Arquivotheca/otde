#: main.cpp:427
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"
msgstr ""
"U¿ycie: %s -install|-setup|-test|-desc|-preview wid\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"

#: main.cpp:430
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Umieszczenie kursora w danym rogu ekranu powoduje:\n"
"                     x = i  brak akcji (ignoruje)\n"
"                     x = s  wygaszenie ekranu\n"
"                     x = l  zablokowanie ekranu\n"
"                    kolejno¶æ: góra-lewo, góra-prawo, dó³-lewo, dó³-prawo\n"
"  -delay num        Okre¶la czas, po jakim uruchamiany jest wygaszacz "
"ekranu\n"
"                     (domy¶lnie 10min)\n"
"  -desc             Wyrzuca opis danego wygaszacza ekranu na stdout\n"
"  -install          Instaluje wygaszacz ekranu\n"
"  -lock             Aby wyj¶æ z wygaszacza ekranu nale¿y podaæ has³o\n"
"  -nice num         Uruchamia z okre¶lon± warno¶ci± nice\n"
"\n"
"  -preview wid      Uruchamia w okre¶lonym XWindow\n"
"  -inroot           Uruchamia w oknie g³ównym\n"
"  -setup            Ustawienia wygaszacza ekranu\n"
"  -test             Uruchamia wygaszacz ekranu natychmiast\n"

