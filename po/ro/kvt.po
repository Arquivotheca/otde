#: main.C:1435
msgid "Permitted arguments are:\n"
msgstr "Argumentele permise sunt:\n"

#: main.C:1436
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <comanda> <arg> ...    executa comanda cu arg - trebuie sa fie ultimul "
"argument\n"

#: main.C:1437
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <nume>           specifica serverul X de afiºare\n"

#: main.C:1438
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>       geometria iniþialã a ferestrei\n"

#: main.C:1439
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr ""
"-print-pipe <nume>        specificã conexiunea pentru imprimanta vt100\n"

#: main.C:1440
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <culoare>          culoare fundal\n"

#: main.C:1441
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <culoare>          culoare font\n"

#: main.C:1442
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <nume_font>      font normal\n"

#: main.C:1443
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack             dezactiveazã - Qt font bug workaround -\n"

#: main.C:1444
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <dimensiune>     tiny, small, normal, large, huge\n"

#: main.C:1445
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    seteazã kvt sã emuleze o consolã de linux\n"

#: main.C:1446
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               fãrã barã de meniu\n"

#: main.C:1447
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             fãrã barã de defilare\n"

#: main.C:1448
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <text>                 titlul ferestrei kvt \n"
"                          (Demodat. Folosiþi -caption în loc)\n"

#: main.C:1450
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Tipul de terminal. Implicit este xterm.\n"

#: main.C:1451
msgid "-C                        Capture system console message\n"
msgstr "-C                        Captureazã mesajele consolei de sistem\n"

#: main.C:1452
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <text>                 numele icoanei\n"

#: main.C:1453
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        ruleazã în modul 7 bit\n"
"-8                        ruleazã în modul 8 bit\n"

#: main.C:1455
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       porneºte shell-ul kvt ca un shell de login\n"
"-ls-                      porneºte shell-ul kvt ca un shell non-login "
"(implicit)\n"

#: main.C:1457
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arg>               trateazã Meta cu prefix ESCAPE, al 8-lea bit "
"setat,\n"
"                          sau ignorã\n"

#: main.C:1458
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr ""
"-sl <numãr>               numãrul de linii de defilare înapoi care se "
"salveazã\n"

#: main.C:1459
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <keysym>          foloseºte alt-keysym pt. a defila în sus prin "
"buffer\n"
"-pagedown <keysym>        foloseºte alt-keysym pt. a defila în jos prin "
"buffer\n"

