#: main.C:1326
msgid "Permitted arguments are:\n"
msgstr "Eblaj argumentoj estas:\n"

#: main.C:1327
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <komando> <arg> ...    lanæu komandon kun arg-oj, devas esti lasta "
"argumento\n"

#: main.C:1328
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <nomo>          indikas la ekranon (X-Servilo)\n"

#: main.C:1329
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>      la komenca formato de la Vt-fenestro\n"

#: main.C:1330
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <nomo>       indikas dukton por vt100-presilo\n"

#: main.C:1331
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt bg <koloro>          fona koloro\n"

#: main.C:1332
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt fg <koloro>          teksta koloro\n"

#: main.C:1333
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <tiparnomo>     normala tiparo\n"

#: main.C:1334
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack            malaktivigu tipar-erarkorekton por Qt\n"

#: main.C:1335
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <grand>         tiny, small, normal, large, huge\n"

#: main.C:1336
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr ""
"-linux                   konfiguru Kvt-on por imiti Linuksan terminalon\n"

#: main.C:1337
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar              kaþu la menulistelon\n"

#: main.C:1338
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar            kaþu la þovilon\n"

#: main.C:1339
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <teksto>               teksto en titollistelo \n"
"                          (ne plu uzata, Uzu -caption anstataýe)\n"

#: main.C:1341
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>               Terminaltipo. Implicita estas xterm.\n"

#: main.C:1342
msgid "-C                        Capture system console message\n"
msgstr "-C                       Kaptu mesaøojn de la operaciuma terminalo\n"

#: main.C:1343
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <teksto>              nomo en piktogramo aý piktograma fenestro\n"

#: main.C:1344
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        rulu en 7bita modo\n"
"-8                        rulu en 8bita modo\n"

#: main.C:1346
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       lanæu Kvt kiel salutterminalo\n"
"-ls-                      lanæu Kvt ne kiel salutterminalo\n"

#: main.C:1348
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arg>               traktu Meta-klavon kun ESCAPE-prefikso, 8a bito aý "
"ignoru\n"

#: main.C:1349
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <n>                  sekurigu n liniojn en revid-bufro\n"

#: main.C:1350
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <klavo>           uzu Alt+klavo por moviøi supren tra la bufro\n"
"-pagedown <klavo>         uzu Alt+klavo por moviøi malsupren tra la bufro\n"

#: main.C:1354
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
