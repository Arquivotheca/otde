msgstr "Shadow passwords"

#: main.cpp:470
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"
msgstr ""
"Uso: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"

#: main.cpp:473
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Avvicinando il cursore all'angolo si \n"
"                             eseguono le seguenti azioni:\n"
"                     x = i  ignora\n"
"                     x = s  salvaschermo (senza richiesta password)\n"
"                     x = l  salvaschermo (con richiesta password)\n"
"                    nell'ordine: alto-sinistra, alto-destra,\n"
"                                 basso-sinistra, basso-destra\n"
"  -delay num        Tempo di attesa prima che il salvaschermo\n"
"                     parta  (predefiniti 10 minuti)\n"
"  -desc             Mostra a video la descrizione del salvaschermo\n"
"  -install          Installa salvaschermo\n"
"  -lock             Richiede la password per uscire dal salvaschermo\n"
"  -nice num         Esegue il salvaschermo con priorita` (num)\n"
"\n"
"  -preview wid      Esegue il salvaschermo in finestra\n"
"  -inroot           Esegue il salvaschermo sulla finestra "
"principale(sfondo)\n"
"  -setup            Configura il salvaschermo\n"
"  -test             Esegue il salvaschermo immediatamente\n"

