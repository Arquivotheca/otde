#: main.C:1160
msgid "Permitted arguments are:\n"
msgstr "Gli argomenti consentiti sono:\n"

#: main.C:1161
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <comando> <arg>  ... esegue  il comando con arg - deve essere l'ultimo "
"argomento\n"

#: main.C:1162
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <nome>          specifica il nome del display (server)\n"

#: main.C:1163
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>      le dimensioni iniziali della finestra\n"

#: main.C:1164
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <nome>       specifica il pipe per le stampanti vt100\n"

#: main.C:1165
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <colore>\t\t colore di sfondo\n"

#: main.C:1166
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <colore>\t\t colore primo piano\n"

#: main.C:1167
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <nome font>\t caratteri normali\n"

#: main.C:1168
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <dimensione>\t tiny, small, normal, large, huge\n"

#: main.C:1169
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux\t\t\t imposta kvt come console linux\n"

#: main.C:1170
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar\t\t nasconde la barra menu\n"

#: main.C:1171
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar\t\t nasconde le barre di scorrimento\n"

#: main.C:1172
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <testo>\t\t   testo nella barra titolo\n"
"\t\t\t   (Obsolet. Usa -caption al suo posto)\n"

#: main.C:1174
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>\t\t Tipo di terminal.Quello di default ee xterm.\n"

#: main.C:1175
msgid "-C                        Capture system console message\n"
msgstr "-C\t\t\t Cattura i messagi di sistema della console\n"

#: main.C:1176
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <testo>\t\t nome in icona o finestra icona\n"

#: main.C:1177
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        esegue in modalitaa 7 bit\n"
"-8                        esegue in modalitaa 8 bit\n"

#: main.C:1179
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""

#: main.C:1181
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""

#: main.C:1182
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr ""

#: main.C:1183
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""

#: main.C:1187
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
