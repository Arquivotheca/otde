#: main.C:1326
msgid "Permitted arguments are:\n"
msgstr "Sallitut komentorivioptiot ovat:\n"

#: main.C:1327
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <komento> <arg> ...    suorita komento argumenteilla - pitää olla "
"viimeinen\n"

#: main.C:1328
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <nimi>           määrittele näytön nimi (palvelin)\n"

#: main.C:1329
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <muoto>      aloitusmuoto ikkunalle\n"

#: main.C:1330
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <nimi>        määrittele putki VT-100 tulostimelle\n"

#: main.C:1331
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <väri>             taustaväri\n"

#: main.C:1332
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <väri>             edustaväri\n"

#: main.C:1333
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <kirjasinnimi>   normaalikirjasin\n"

#: main.C:1334
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack             poista Qt fonttivirheen kierto\n"

#: main.C:1335
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <koko>           tiny, small, normal, large, huge\n"

#: main.C:1336
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    aseta kvt matkimaan linuxin konsolia\n"

#: main.C:1337
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               piilota valikkorivi\n"

#: main.C:1338
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             piilota vierityspalkki\n"

#: main.C:1339
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <teksti>               Terminaalin otsikko \n"
"                          (Vanha. Käytä -caption:ia)\n"

#: main.C:1341
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Terminaalin tyyppi. Oletus on xterm.\n"

#: main.C:1342
msgid "-C                        Capture system console message\n"
msgstr "-C                        Kaappaa järjestelmän konsoliviestit\n"

#: main.C:1343
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <teksti>               nimi ikonin yhteydessä\n"

#: main.C:1344
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        aja 7 bittisessä tilassa\n"
"-8                        aja 8 bittisessä tilassa\n"

#: main.C:1346
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       taseta kvt:n komentotulkki login tilaan.\n"
"-ls-                      aseta kvt:n komentotulkki ei-login tilaan "
"(oletus)\n"

#: main.C:1348
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arg>               Käsittele Meta ESCAPE etumerkillä, 8THBIT "
"asetettu, tai ohita\n"

#: main.C:1349
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <määrä>               kuinka monta riviä säilytetään puskurissa\n"

#: main.C:1350
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <näpp.symb.>      käytä alt + näppäintä puskurin ylös selaamiseen\n"
"-pagedown <näpp.symb.>    käytä alt + näppäintä puskurin ylös selaamiseen\n"

#: main.C:1354
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tgreek kbd = ELOT 928 (oletus)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
