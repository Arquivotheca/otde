#: main.C:1326
msgid "Permitted arguments are:\n"
msgstr "Argumentos aceitos são:\n"

#: main.C:1327
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
       "-e <comando> <arg> ...    executar comando com arg - deve ser o último "
       "argumento\n"

#: main.C:1328
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <nome>           especificar a tela (servidor)\n"

#: main.C:1329
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>       a geometria inicial da janela do vt\n"

#: main.C:1330
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <nome>        especificar pipe para impressora de vt100\n"

#: main.C:1331
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <cor>              cor de fundo\n"

#: main.C:1332
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <cor>              cor das letras\n"

#: main.C:1333
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <nome da fonte>   fonte normal\n"

#: main.C:1334
msgid  "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack             desliga desvio do bug de fontes do Qt\n"

#: main.C:1335
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <tamanho>        tiny, small, normal, large, huge\n"

#: main.C:1336
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr ""
       "-linux                    configura o kvt para imitar o console linux\n"

#: main.C:1337
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               esconde a barra de menu\n"

#: main.C:1338
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             esconde a barra de arrasto\n"

#: main.C:1339
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
       "-T <texto>                texto no título da janela\n"
       "                          (Obsoleto. Use -caption)\n"

#: main.C:1341
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>               Tipo de terminal. Padrão é xterm.\n"

#: main.C:1342
msgid "-C                        Capture system console message\n"
msgstr "-C                        Capturar mensagens do console do sistema\n"

#: main.C:1343
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <texto>                nome no ícone ou janela de ícone\n"

#: main.C:1344
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
       "-7                        rodar em modo 7 bits\n"
       "-8                        rodar em modo 8 bits\n"

#: main.C:1346
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
       "-ls                       iniciar kvt como shell de login\n"
       "-ls-                      initiar kvt não como shell de login (padrão)\n"

#: main.C:1348
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
       "-meta <arg>               tratar Meta com prefixo de ESCAPE, 8THBIT "
       "colocado, ou ignorar\n"

#: main.C:1349
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr ""
       "-sl <número>              número de linhas gravadas no buffer de "
       "scroll-back\n"

#: main.C:1350
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
       "-pageup <keysym>          utilizar alt-keysym para subir no buffer de "
       "scroll-back\n"
       "-pagedown <keysym>        utilizar alt-keysym para descer no buffer de "
       "scroll-back\n"

#: main.C:1354
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr "-grk9\t\tgreek kbd = ELOT 928 (padrão)\n"
       "-grk4\t\tgreek kbd = IBM 437\n"
