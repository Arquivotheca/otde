msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <colour>           barva textu\n"

#: main.C:1333
msgid "-vt_font <fontname>       normal font\n"
msgstr "vt_font <fontname>        normalní font\n"

