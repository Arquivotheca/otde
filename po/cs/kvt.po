msgid "-display <name>           specify the display (server)\n"
msgstr "-display <jméno>          specifikace obrazovky (serveru)\n"

#: main.C:1329
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>       poèáteèní geometrie okna terminálu\n"

#: main.C:1330
