msgstr "-print-pipe <name>        specifikuje pipe pro vt100 tiskárnu\n"

#: main.C:1403
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <colour>           barva pozadí\n"

#: main.C:1404
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <colour>           barva textu\n"

#: main.C:1405
msgid "-vt_font <fontname>       normal font\n"
msgstr "vt_font <fontname>        normalní font\n"

#: main.C:1406
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack             vypnout záplatu chyby výbìru fontu v QT\n"

#: main.C:1407
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <size>           tiny, small, normal, large, huge\n"

#: main.C:1408
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    nastaví kvt, aby imitoval linux konsoli\n"

#: main.C:1409
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               skryje menu\n"

#: main.C:1410
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             skryje pusunovací li¹tu\n"

#: main.C:1411
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
