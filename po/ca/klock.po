#: main.cpp:476
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"
msgstr ""
"Us: %s -install|-setup|-test|-desc|-preview mid|-inroot\n"
"    [-corners xxxx] [-delay num] [-lock] [-nice num]\n"

#: main.cpp:479
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     En situar el cursor al racó s'efectua l'acció:\n"
"                     x = i  cap acció (ignorar)\n"
"                     x = s  salvar pantalla\n"
"                     x = l  bloquejar pantalla\n"
"                    ordre: dalt-esq, dalt-dreta, baix-esq, baix-dreta\n"
"  -delay num        Temps d'inactivitat abans d'engegar el salvapantalles\n"
"                      (10 minuts per omissió)\n"
"  -desc             Imprimir la descripció del salvapantalles a la sortida\n"
"  -install          Instal·lar el salvapantalles\n"
"  -lock             Demanar la paraula de pas per aturar el salvapantalles\n"
"  -nice num         Executar amb el valor nice especificat\n"
"\n"
"  -preview wid      Executar a la finestra X especificada\n"
"  -inroot           Executar a la finestra X\n"
"  -setup            Valors del salvapantalles\n"
"  -test             Invocar el salvapantalles immediatament\n"

