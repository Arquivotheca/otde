#: main.cpp:580
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-allow-root] [-nice num]\n"
msgstr ""
"Brug: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"

#: main.cpp:583
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -allow-root       Accept root password to unlock\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Placering afmarkør i hjørnet gør følgende:\n"
"                     x = i  intet (ignorer)\n"
"                     x =s  gem skærmen\n"
"                     x = l  lås skærmen\n"
"              rækkefølge: top-venstre, top-højre, bund-venstre,bund-højre\n"
"  -delay num      Tiden det tager før skærmskåneren begynder\n"
"                     starter  (standard 10min)\n"
"  -desc             Tryk skærmskånerens beskrivelse til standard output\n"
"  -install          Installer skærmskåner\n"
"  -lock             Kræv kodeord for at standse skærmskåner\n"
"  -allow-root       Accepter root kodeord for at låse op\n"
"  -nice num         Kør med specifikke gode værdier\n"
"\n"
"  -preview wid     Kør i specificeret Xvindue\n"
"  -inroot           Kør i root Xvindue\n"
"  -setup            Skærmskåner opsætning\n"
"  -test             Start skærmskåneren med det samme\n"

