#: main.cpp:469
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"
msgstr ""
"Gebruik: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"

#: main.cpp:472
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Plaatsen van cursor in hoek laat handeling uitvoeren:\n"
"                     x = i  geen handeling ('ignore')\n"
"                     x = s  bewaar scherm ('save')\n"
"                     x = l  sluit scherm ('lock')\n"
"                    volgorde: top-links,top-rechts,bodem-links,bodem-rechts\n"
"  -delay num        Onaangeroerde periode voordat de screensaver\n"
"                     start  (standaard 10min)\n"
"  -desc             Druk de screen saver's beschrijving af op stdout\n"
"  -install          Installeer screen saver\n"
"  -lock             Vraag wachtwoord om screen saver te stoppen\n"
"  -nice num         Voer uit met gespecificeerde 'nice' waarde\n"
"\n"
"  -preview wid      Voer uit in gespecificeerd Venster\n"
"  -inroot           Voer uit op het achtergrond vlak\n"
"  -setup            Stel de screen saver in\n"
"  -test             Start de screen saver onmiddellijk\n"

