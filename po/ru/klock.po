#: main.cpp:469
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"
msgstr ""
"Использование: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay кол] [-lock] [-nice кол]\n"

#: main.cpp:472
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Перемещение курсора в угол производит следующие "
"действия:\n"
"                     x = i  ничего (игнорировать)\n"
"                     x = s  сохранять экран\n"
"                     x = l  закрыть экран\n"
"                    порядок: верх-лево, верх-право, вниз-лево, вниз-право\n"
"  -delay num        Время простоя после которого запускается хранитель\n"
"                     (по умолчанию 10 мин)\n"
"  -desc             Напечатать описание хранитель на стандартный выход\n"
"  -install          Установить хранитель\n"
"  -lock             Необходим пароль, чтобы прервать хранитель\n"
"  -nice num         Запускаться с указанным приоритетом\n"
"\n"
"  -preview wid      Запускаться в указанном окне\n"
"  -inroot           Запускаться в главном окне\n"
"  -setup            Настройка хранителя\n"
"  -test             Запустить хранитель немедленно\n"

