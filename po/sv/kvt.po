#: main.C:1437
msgid "Permitted arguments are:\n"
msgstr "Tillåtna argument är:\n"

#: main.C:1438
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <kommando> <arg> ...   kör kommando med argumenten arg\n"
"                          OBS: detta måste vara det sista argumentet till "
"kvt\n"

#: main.C:1439
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <namn>           ange bildskräm (server)\n"

#: main.C:1440
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>       initial storlek på vt-fönstret\n"

#: main.C:1441
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <namn>        ange rör för vt100-skrivare\n"

#: main.C:1442
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <färg>             bakgrundsfärg\n"

#: main.C:1443
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <färg>             förgrundsfärg\n"

#: main.C:1444
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <typsnittsnamn>  normalt typsnitt\n"

#: main.C:1445
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack             stäng av fix för typsnittsbug i Qt\n"

#: main.C:1446
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <storlek>        tiny, small, normal, large, huge\n"

#: main.C:1447
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    ställ in kvt att likna en linux-konsol\n"

#: main.C:1448
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               göm menyraden\n"

#: main.C:1449
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             göm rullningslisten\n"

#: main.C:1450
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <text>                 text i fönstrets namnlist\n"
"                          (Föråldrad. Använd -caption i stället)\n"

#: main.C:1452
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Terminaltyp. Standard är xterm.\n"

#: main.C:1453
msgid "-C                        Capture system console message\n"
msgstr "-C                        Fånga meddelanden till systemkonsolen\n"

#: main.C:1454
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <text>                 namn i ikon eller ikonfönster\n"

#: main.C:1455
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        kör i 7-bitarsläge\n"
"-8                        kör i 8-bitarsläge\n"

#: main.C:1457
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       starta kvt:s skal som ett inloggningsskal\n"
"-ls-                      starta ej kvt:s skal som inloggningsskal "
"(standard)\n"

#: main.C:1459
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arg>               ange hur Meta-tangenten hanteras:\n"
"                          ESCAPE - skicka escape-tecken som prefix\n"
"                          8THBIT - sätt bit 8\n"
"                          ignore - ignorera\n"

#: main.C:1460
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <antal>               spara antal rader i scrollback-bufferten\n"

#: main.C:1461
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <keysym>          använd snabbtangent alt-keysym för att scrolla "
"upp\n"
"-pagedown <keysym>        använd snabbtangent alt-keysym för att scrolla "
"ner\n"

#: main.C:1465
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tgrekiskt tangentbord = ELOT 928 (standard)\n"
"-grk4\t\tgrekiskt tangentbord = IBM 437\n"
