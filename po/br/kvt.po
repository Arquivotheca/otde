#: main.C:1437
msgid "Permitted arguments are:\n"
msgstr "An arventennoù aotreet zo :\n"

#: main.C:1438
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <command> <arv> ...    sevbeniñ urzhiad gant arv - a rank bezañ an\n"
"                          arventenn diwezhañ\n"

#: main.C:1439
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <name>           resisaat an diskouez (titourer)\n"

#: main.C:1440
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>       mentrezh ar prenestr vt kentañ\n"

#: main.C:1441
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <name>        resisaat ar gorzenn evit ar voulerez vt100\n"

#: main.C:1442
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <colour>           liv drekleur\n"

#: main.C:1443
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <colour>           liv rakleur\n"

#: main.C:1444
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <anv nodrezh>    nodrezh boas\n"

#: main.C:1445
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack             dizoberia kempennadur draen nodrezh Qt\n"

#: main.C:1446
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <ment>           tiny, small, normal, large, huge\n"

#: main.C:1447
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    keflunia kvt da varmouzañ letrin linux\n"

#: main.C:1448
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               kuzat barrenn al lañser\n"

#: main.C:1449
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             kuzaat ar varrenn dibunañ\n"

#: main.C:1450
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <testenn>              testenn er barrenn-ditl ar prenestr\n"
"                          (Dic'hiziet. Arverit -caption kentoc'h)\n"

#: main.C:1452
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Rizh an termenell. xterm dre ziouer.\n"

#: main.C:1453
msgid "-C                        Capture system console message\n"
msgstr "-C                        Tapout kemennadoù letrin ar reizhad\n"

#: main.C:1454
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <testenn>              anv an arlun pe prenestr arlun\n"

#: main.C:1455
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        seveniñ mod 7 bit\n"
"-8                        seveniñ mod 8 bit\n"

#: main.C:1457
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       loc'hañ shell kvt evel ul login shell\n"
"-ls-                      loc'hañ shell kvt evel un nann-login shell\n"
"                          (dre ziouer)\n"

#: main.C:1459
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arv>               merañ Meta evel araogenn ESCAPE, 8vet BIT\n"
"                          termenet, pe na ober van\n"

#: main.C:1460
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <niver>               enrollañ <niver> linenn er skurzer dibunañ\n"

#: main.C:1461
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <stokarouez>      arverañ berradenn alt-stokarouez evit dibunañ\n"
"                          davet gorre ar skurzer\n"
"-pagedown <stokarouez>    arverañ berradenn alt-stokarouez evit dibunañ\n"
"                          davet traoñ ar skurzer\n"

#: main.C:1465
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tstokellaoueg gresian = ELOT 928 (dre ziouer)\n"
"-grk4\t\tstokellaoueg gresian = IBM 437\n"
