#: main.C:1398
msgid "Permitted arguments are:\n"
msgstr "Los parámetros permitidos son:\n"

#: main.C:1399
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <comando> <arg> ...    ejecutar comando con parámetro - tiene que ser el "
"último parámetro\n"

#: main.C:1400
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <name>           especificar la pantalla (servidor)\n"

#: main.C:1401
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>       la geometría inicial del terminal\n"

#: main.C:1402
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <name>        especifica pipa para impresora vt100\n"

#: main.C:1403
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <colour>           color del fondo\n"

#: main.C:1404
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <colour>           color del texto\n"

#: main.C:1405
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <fontname>       fuente normal\n"

#: main.C:1406
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr ""
"-no_font_hack             apagar código contra error de fuentes en Qt\n"

#: main.C:1407
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr ""
"-vt_size <size>           muy pequeña, pequeña, mediana, grande, enorme\n"

#: main.C:1408
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    imita la consola linux\n"

#: main.C:1409
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               esconder la barra de menú\n"

#: main.C:1410
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             esconder la barra de scroll\n"

#: main.C:1411
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <text>                 texto en la barra de título de la ventana\n"
"                          (Obsoleto. Mejor use -caption)\n"

#: main.C:1413
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                tipo de terminal (xterm por defecto).\n"

#: main.C:1414
msgid "-C                        Capture system console message\n"
msgstr ""
"-C                        capturar mensajes de la consola del sistema\n"

#: main.C:1415
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <text>                 nombre del ícono o ventana del ícono\n"

#: main.C:1416
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        usar modo de 7 bits\n"
"-8                        usar modo de 8 bits\n"

#: main.C:1418
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       iniciar kvt con shell de login\n"
"-ls-                      iniciar kvt con shell sin login (por defecto)\n"

#: main.C:1420
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr "-meta <arg>               tratar Meta como ESCAPE, 8THBIT o ignorar\n"

#: main.C:1421
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <número>              guardar número de líneas pasadas\n"

#: main.C:1422
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <keysym>          usar tecla alt-keysym para moverse hacia atrás\n"
"-pagedown <keysym>        usar tecla alt-keysym para moverse haca adelante\n"

#: main.C:1426
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tgreek kbd = ELOT 928 (por defecto)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
