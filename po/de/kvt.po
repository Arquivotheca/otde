#: main.C:1437
msgid "Permitted arguments are:\n"
msgstr "Zulässige Parameter:\n"

#: main.C:1438
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <Kommando> <Arg> ...   Kommando mit Argumenten ausführen - muß letzter "
"Parameter sein\n"

#: main.C:1439
msgid "-display <name>           specify the display (server)\n"
msgstr ""
"-display <name>           Name des Rechners, auf dem das Terminal-Fenster "
"erscheint\n"

#: main.C:1440
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <Größe>      Anfängliche Fenstergröße\n"

#: main.C:1441
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <Name>        gibt die Pipe für den vt100 Printer an\n"

#: main.C:1442
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <Farbe>            Hintergrundfarbe\n"

#: main.C:1443
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <Farbe>            Vordergrundfarbe\n"

#: main.C:1444
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <Schrift>        Standard-Schrift\n"

#: main.C:1445
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack             schaltet Qt's font bug Workaround aus\n"

#: main.C:1446
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <size>           tiny, small, normal, large, huge\n"

#: main.C:1447
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    Kvt wie Linux-Konsole einstellen\n"

#: main.C:1448
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               Menüleiste verstecken\n"

#: main.C:1449
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             Rollbalken verstecken\n"

#: main.C:1450
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <text>                 Titeltext\n"
"                          (Veraltet. Benutze -caption anstatt)\n"

#: main.C:1452
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Terminaltyp, Voreinstellung ist \"xterm\"\n"

#: main.C:1453
msgid "-C                        Capture system console message\n"
msgstr "-C                        Meldungen für die Systemkonsole anzeigen\n"

#: main.C:1454
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <text>                 Text für Icon\n"

#: main.C:1455
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                         7-Bit-Mode\n"
"-8                         8-Bit-Mode\n"

#: main.C:1457
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       Login-Shell starten\n"
"-ls-                      Keine Login-Shell starten (standard)\n"

#: main.C:1459
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr "-meta                     ???\n"

#: main.C:1460
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <Zahl>                Anzahl der zu speichernden Zeilen\n"

#: main.C:1461
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <Taste>           <Alt>-<Taste> zum Rückwärtsrollen benutzen\n"
"-pagedown <Taste>         <Alt>-<Taste> zum Vorwärtsrollen benutzen\n"

#: main.C:1465
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tgreek kbd = ELOT 928 (standard)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
